        display: block;
    }

        /* Hide table headers (but not display: none;, for accessibility) */
        thead tr {
            position: absolute;
            top: -9999px;
            left: -9999px;
        }

    tr {
        border: 1px solid #ccc;
    }

    td {
        /* Behave  like a "row" */
        border: none;
        border-bottom: 1px solid #eee;
        position: relative;
        padding-left: 50%;
    }

        td:before {
            /* Now like a table header */
            /*position: absolute;
		 Top/left values mimic padding */
            top: 6px;
            left: 6px;
            width: 45%;
            padding-right: 10px;
            white-space: nowrap;
            font-weight: bold;
        }

    .table-resultados > tbody > tr > td:nth-of-type(1):before { content: "Tipo de prova"; }
    .table-resultados > tbody > tr > td:nth-of-type(2):before { content: "Curso"; }
    .table-resultados > tbody > tr > td:nth-of-type(3):before { content: "Opção"; }
    .table-resultados > tbody > tr > td:nth-of-type(4):before { content: "Situação"; }
    .table-resultados > tbody > tr > td:nth-of-type(5):before { content: "Comentário"; }
    .table-resultados > tbody > tr > td:nth-of-type(6):before { content: "Classificação no Curso"; }

    .table-avaliacoes > tbody > tr > td:nth-of-type(1):before { content: "Avaliação"; }
    .table-avaliacoes > tbody > tr > td:nth-of-type(2):before { content: "Nota"; }

    .table-inscricoes > tbody > tr > td:nth-of-type(1):before { content: "Processo Seletivo: "; }
    .table-inscricoes > tbody > tr > td:nth-of-type(2):before { content: "Nome do Inscrito: "; }
    .table-inscricoes > tbody > tr > td:nth-of-type(3):before { content: "Nº de Inscrição:  "; }
    .table-inscricoes > tbody > tr > td:nth-of-type(4):before { content: "Situação: "; }
        
}

